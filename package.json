{
    "name": "@mdrajibul/nodejs-boilerplate",
    "version": "1.0.0",
    "description": "NodeJs, ExpressJs, webpack boilerplate",
    "main": "dist",
    "types": "dist/index.d.ts",
    "scripts": {
        "start:dev": "set NODE_ENV=development && webpack",
        "start": "set NODE_ENV=development && nodemon build/index.js",
        "prestart": "npm run build",
        "build": "del-cli dist && webpack",
        "test": "jest",
        "prettier": "prettier --write src/**/*.{js,jsx,ts,tsx,css}",
        "lint": "tslint --project tsconfig.json",
        "lint:fix": "tslint --project tsconfig.json --fix"
    },
    "author": "MD RAJIB UL ISLAM<mdrajibul@gmail.com>",
    "license": "ISC",
    "files": [
        "dist"
    ],
    "dependencies": {
        "cors": "^2.8.5",
        "express": "^4.17.1"
    },
    "devDependencies": {
        "@types/express": "^4.17.11",
        "@types/jest": "^26.0.23",
        "@types/node": "^12.11.1",
        "@types/supertest": "^2.0.10",
        "copy-webpack-plugin": "^8.1.1",
        "del-cli": "^3.0.0",
        "file-loader": "^6.2.0",
        "jest": "^26.6.3",
        "nodemon": "^2.0.0",
        "prettier": "^1.18.2",
        "supertest": "^4.0.2",
        "ts-jest": "^26.3.0",
        "ts-loader": "^8.0.3",
        "tslint": "^5.14.0",
        "tslint-config-prettier": "^1.18.0",
        "tslint-plugin-prettier": "^2.0.1",
        "typescript": "^3.8.3",
        "webpack": "^5.37.0",
        "webpack-cli": "^4.7.0",
        "webpack-node-externals": "^2.5.2"
    },
    "keywords": [
        "nodeJs",
        "expressJs",
        "webpack",
        "prettier",
        "boilerplate",
        "seed"
    ]
}
